class Dataset:

    
    def__init__(self, DATASET, CLIN DSET NAME, PK, NAME, DOB, SEX, WEIGHT, HEIGT, COMMENT, PK ORIGIN):
        self.DATASET = DATASET
        self.CLIN DSET NAME = CLIN DSET NAME
        self.PK = PK
        self.NAME = NAME
        self.DOB = DOB
        self.SEX = SEX
        self.WEIGHT = WEIGHT
        self.HEIGT = HEIGT
        self.COMMENT = COMMENT
        self.PK ORIGIN = PK ORIGIN


          def myfunc(self):
